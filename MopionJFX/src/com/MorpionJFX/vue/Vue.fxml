<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" stylesheets="@../ressources/application.css"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.MorpionJFX.controleur.MainController">
	<children>
		<VBox prefHeight="400.0" prefWidth="600.0">
			<children>
				<Label alignment="CENTER" contentDisplay="CENTER"
					prefHeight="88.0" prefWidth="595.0" text="BIENVENUE"
					textAlignment="CENTER">
					<font>
						<Font name="Nakula" size="50.0" />
					</font>
					<VBox.margin>
						<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
					</VBox.margin>
				</Label>
				<HBox id="Hbox-border" prefHeight="100.0" prefWidth="200.0">
					<children>
						<Label prefHeight="99.0" prefWidth="286.0"
							text="Contre qui voulez vous jouez ?" translateX="20.0">
							<font>
								<Font name="Lato Heavy" size="20.0" />
							</font>
						</Label>
						<ComboBox prefHeight="33.0" prefWidth="171.0"
							translateX="55.0" translateY="35.0" />
					</children>
					<VBox.margin>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</VBox.margin>
				</HBox>
				<Button fx:id="nvPartie" alignment="CENTER"
					contentDisplay="CENTER" mnemonicParsing="false"
					onAction="#lancerPartie" prefHeight="46.0" prefWidth="224.0"
					text="Nouvelle partie" translateX="180.0">
					<VBox.margin>
						<Insets />
					</VBox.margin>
					<font>
						<Font name="Lato Heavy" size="20.0" />
					</font>
				</Button>
				<Button fx:id="ldPartie" alignment="CENTER"
					contentDisplay="CENTER" mnemonicParsing="false"
					onAction="#loadPartie" prefHeight="48.0" prefWidth="226.0"
					text="Continuer une partie" translateX="180.0" translateY="20.0">
					<VBox.margin>
						<Insets />
					</VBox.margin>
					<font>
						<Font name="Lato Heavy" size="20.0" />
					</font>
				</Button>
			</children>
		</VBox>
	</children>
</Pane>
